{
  "name": "inspiro-gatsby-starter",
  "private": true,
  "description": "An advanced starter to get up and developing quickly with Gatsby",
  "version": "0.2.0",
  "author": "Gabesz Rozsa & Gergo Muller <cheers@inspiro.studio>",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lint": "eslint src/**/*.{js,jsx}",
    "dev": "yarn run develop",
    "dev:local": "dotenv netlify dev",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "postinstall": "netlify-lambda install"
  },
  "dependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^9.8.5",
    "axios": "^0.20.0",
    "clsx": "^1.1.1",
    "cssnano": "^4.1.10",
    "formik": "^2.1.5",
    "gatsby": "^2.24.4",
    "gatsby-background-image": "^1.1.2",
    "gatsby-cli": "^2.12.62",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-intl": "^0.3.3",
    "gatsby-plugin-manifest": "^2.4.18",
    "gatsby-plugin-netlify": "^2.3.11",
    "gatsby-plugin-offline": "^3.2.18",
    "gatsby-plugin-postcss": "^2.3.11",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-sharp": "^2.6.19",
    "gatsby-plugin-sitemap": "^2.4.11",
    "gatsby-source-filesystem": "^2.3.20",
    "gatsby-transformer-sharp": "^2.5.11",
    "moment": "^2.27.0",
    "netlify-lambda": "^2.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-calendar": "^3.1.0",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-loader-spinner": "^3.1.14",
    "react-multi-carousel": "^2.5.5",
    "react-scroll": "^1.8.1",
    "tailwindcss": "^1.5.1",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "prettier": "^1.18.2"
  },
  "keywords": [
    "gatsby"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  }
}
